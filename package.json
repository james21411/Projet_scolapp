{
  "name": "scolapp",
  "version": "0.1.0",
  "private": true,
  "description": "Application de gestion scolaire avec interface Electron",
  "author": "ScolApp Team",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "dev": "next dev",
    "dev:lan": "next dev --hostname 0.0.0.0",
    "dec:lan": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --publish=never",
    "build-electron": "node scripts/build-electron.js",
    "build-simple": "node scripts/build-simple.js",
    "build-minimal": "node scripts/build-minimal.js",
    "build-local": "node scripts/build-local.js",
    "build-simple-local": "node scripts/build-simple-local.js",
    "create-executable": "node scripts/create-executable.js",
    "create-real-exe": "node scripts/create-real-exe.js",
    "create-launcher": "node scripts/create-launcher-bat.js",
    "create-professional": "node scripts/create-professional-launcher.js",
    "create-deployment": "node scripts/create-deployment-package.js",
    "create-minimal": "node scripts/create-minimal-deployment.js",
    "cleanup-dist": "node scripts/cleanup-dist.js",
    "create-source-only": "node scripts/create-source-only-deployment.js",
    "cleanup-dist-final": "node scripts/cleanup-dist-final.js",
    "cleanup-database": "node scripts/cleanup-database.js",
    "restore-base-data": "node scripts/restore-base-data.js",
    "prepare-for-client": "node scripts/prepare-for-client.js",
    "test-cleanup": "node scripts/test-cleanup.js"
  },
  "build": {
    "appId": "com.scolapp.desktop",
    "productName": "ScolApp",
    "directories": {
      "output": "dist"
    },
    "files": [
      ".next/**/*",
      "node_modules/**/*",
      "public/electron.js",
      "public/**/*",
      "package.json"
    ],
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icon.png"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "embla-carousel-react": "^8.6.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "firebase": "^10.7.1",
    "framer-motion": "^10.16.16",
    "genkit": "^1.16.1",
    "html2canvas": "^1.4.1",
    "iron-session": "^8.0.4",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.294.0",
    "mysql2": "^3.6.5",
    "next": "14.0.4",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.1",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.8.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jspdf": "^1.3.3",
    "@types/qrcode": "^1.5.5",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "electron": "^27.1.3",
    "electron-builder": "^24.6.4",
    "electron-is-dev": "^2.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "wait-on": "^7.2.0"
  }
}
